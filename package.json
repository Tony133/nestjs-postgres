{
  "name": "nest-postgres",
  "version": "0.0.20",
  "description": "PostgreSQL module for Nest framework (node.js) ðŸ˜»",
  "author": "Tony133",
  "license": "MIT",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:e2e": "jest --config ./tests/jest-e2e.json --detectOpenHandles --forceExit",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch",
    "prerelease": "npm run build",
    "release": "release-it",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "20.3.0",
    "@commitlint/config-angular": "20.3.0",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@nestjs/common": "11.1.11",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.11",
    "@nestjs/platform-fastify": "11.1.11",
    "@nestjs/testing": "11.1.11",
    "@types/jest": "30.0.0",
    "@types/node": "25.0.3",
    "@types/pg": "8.16.0",
    "@types/supertest": "6.0.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "17.0.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "pg": "8.16.3",
    "prettier": "3.7.4",
    "reflect-metadata": "0.2.2",
    "release-it": "19.2.2",
    "rxjs": "7.8.2",
    "supertest": "7.2.2",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.52.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "rxjs": "^6.6.3 || ^7.2.0"
  },
  "lint-staged": {
    "**/*.{ts,json}": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tony133/nestjs-postgres.git"
  }
}
